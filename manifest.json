{
    "manifest_version": 3,
    "name": "History Clear",
    "version": "1.0",
    "description": "Clear browsing history with a single click.",
    "permissions": ["history"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }
}
